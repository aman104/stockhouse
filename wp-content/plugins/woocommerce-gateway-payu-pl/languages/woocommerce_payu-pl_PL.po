#: classes/payu.class.php:125
#@ woocommerce_payu
msgid "Klucz autoryzacji płatności (pos_auth_key)"
msgstr ""

#: classes/payu.class.php:127
#@ woocommerce_payu
msgid "Please enter your POS auth key; this number should be provided by PayU during registration!"
msgstr "Wpisz klucz pos_auth_key w systemie PayU, uzyskany podczas tworzenia punktu płatności."

